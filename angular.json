{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-pipes-lite": {
      "projectType": "library",
      "root": "projects/ngx-pipes-lite",
      "sourceRoot": "projects/ngx-pipes-lite/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngx-pipes-lite/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-pipes-lite/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-pipes-lite/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ngx-pipes-lite/tsconfig.spec.json",
            "polyfills": "projects/ngx-pipes-lite/src/polyfills.ts",
            "main": "projects/ngx-pipes-lite/src/test.ts"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/ngx-pipes-lite/**/*.ts",
              "projects/ngx-pipes-lite/**/*.html"
            ]
          }
        }
      }
    },
    "examples": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "inlineTemplate": true,
          "inlineStyle": true,
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:resolver": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "projects/examples",
      "sourceRoot": "projects/examples/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/examples",
            "index": "projects/examples/src/index.html",
            "main": "projects/examples/src/main.ts",
            "polyfills": "projects/examples/src/polyfills.ts",
            "tsConfig": "projects/examples/tsconfig.app.json",
            "assets": [
              "projects/examples/src/favicon.ico",
              "projects/examples/src/assets"
            ],
            "styles": [
              "projects/examples/src/styles.css",
              "node_modules/axpager/dist/themes/axpager.theme.light.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "examples:build:production"
            },
            "development": {
              "browserTarget": "examples:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "examples:build"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/examples/**/*.ts",
              "projects/examples/**/*.html"
            ]
          }
        }
      }
    },
    "ngx-menubar": {
      "projectType": "library",
      "root": "projects/ngx-menubar",
      "sourceRoot": "projects/ngx-menubar/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngx-menubar/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-menubar/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-menubar/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-menubar/src/test.ts",
            "tsConfig": "projects/ngx-menubar/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-menubar/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/ngx-menubar/**/*.ts",
              "projects/ngx-menubar/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "schematicCollections": [
      "@angular-eslint/schematics"
    ]
  }
}
